   "source": [
    "## Gets the Currency from the Location and makes it a feature"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "25720911",
   "metadata": {},
   "outputs": [],
   "source": [
    "currency = []\n",
    "for location in df[\"Location\"]:\n",
    "    if location == 'London':\n",
    "        currency.append('GBP')\n",
    "    elif location == 'Milan':\n",
    "        currency.append('EUR')\n",
    "    elif location == 'Hong Kong':\n",
    "        currency.append(\"HKD\")\n",
    "    elif location == 'Paris':\n",
    "        currency.append('EUR')\n",
    "    elif location == 'New York':\n",
    "        currency.append('USD')\n",
    "    else:\n",
    "        currency.append(0)\n",
    "df.insert(loc=5, column='Currency', value=currency)"
